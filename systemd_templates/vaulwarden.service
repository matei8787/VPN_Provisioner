[Unit]
Description=VPN Provisioner Service
After=postgres.service
Requires=postgres.service
PartOf=dorb.target

[Service]
Type=oneshot
RemainAfterExit=yes
WorkingDirectory=/opt/services/vaultwarden
ExecStart=/usr/bin/docker-compose up --build -d
ExecStop=/usr/bin/docker-compose down -v

[Install]
WantedBy=dorb.target